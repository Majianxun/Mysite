     <script>

         var speed = 30;  //滚动的速度,数字越小快，前面的var为定义一个变量，是固定的语法

         var tab = document.getElementById("doctorIntroduce");  //获取id为demo的div

         var tab1 = document.getElementById("doctors1");  //获取id为demo1的div

         var tab2 = document.getElementById("doctors2");  //获取id为demo1的div

         tab2.innerHTML = tab1.innerHTML;

         var MyMar = setInterval(Marquee, speed);

         function Marquee() {

             if (tab2.offsetWidth - tab.scrollLeft <= 0) {

                 tab.scrollLeft -= tab1.offsetWidth;

             } else {

                 tab.scrollLeft++;

             }

         }
         tab.onmouseover = function() { clearInterval(MyMar) };
         tab.onmouseout = function() { MyMar = setInterval(Marquee, speed) };
    </script>
